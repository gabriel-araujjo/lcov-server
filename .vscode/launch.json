    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "program": "${workspaceFolder}/index.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "-u",
                "http://localhost:8080",
                "-C",
                "${workspaceFolder}/../example-c"
            ],
            "program": "${workspaceFolder}/bin/upload-report"
        }
    ]
}
