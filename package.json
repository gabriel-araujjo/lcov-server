{
  "name": "node-coverage-server",
  "version": "0.0.2",
  "description": "a server that recieves lcov data and generates a dashboard to view that data",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "tape test/**/*.js",
    "build": "webpack --progress",
    "coverage": "tap test/**/*.js --coverage",
    "send-coverage": "tap test/**/*.js --coverage-report=text-lcov | ./bin/node-coverage-cli.js --url https://node-coverage-server.herokuapp.com",
    "generate-docs": "jsdoc -c jsdoc.json",
    "lint": "eslint .",
    "dev": "webpack-dev-server --hot --port 5000",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Gabriel J. Csapo <gabecsapo@gmail.com>",
  "bin": {
    "node-coverage-cli": "./bin/node-coverage-cli.js",
    "node-coverage-server": "./bin/node-coverage-server.js"
  },
  "license": "ISC",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.26.1",
    "docdash": "^0.4.0",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "getstorybook": "^1.7.0",
    "jsdoc": "^3.4.3",
    "moment": "^2.17.1",
    "psychic-ui": "^0.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "style-loader": "^0.13.1",
    "tap": "^10.0.2",
    "tape": "^4.6.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "whatwg-fetch": "^2.0.1"
  },
  "dependencies": {
    "mongodb": "^2.2.21",
    "path-to-regexp": "^1.7.0",
    "commander": "^2.9.0",
    "git-url-parse": "^6.2.0",
    "openbadge": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielcsapo/node-coverage-server.git"
  },
  "engines": {
    "node": ">= 6"
  },
  "bugs": {
    "url": "https://github.com/gabrielcsapo/node-coverage-server/issues"
  },
  "homepage": "https://github.com/gabrielcsapo/node-coverage-server#readme"
}
